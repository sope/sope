{
  "name": "vjit",
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./core": {
      "import": "./dist/core.js",
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    },
    "./ipc": {
      "import": "./dist/ipc.js",
      "types": "./dist/ipc.d.ts",
      "default": "./dist/ipc.js"
    }
  },
  "typesVersions": {
    "*": {
      "core": [
        "./dist/vite.d.ts"
      ],
      "ipc": [
        "./dist/ipc.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@whatwg-node/disposablestack": "^0.0.6",
    "nanoid": "^5.1.5",
    "path-to-regexp": "^8.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
