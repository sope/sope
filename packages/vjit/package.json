{
  "name": "vjit",
  "version": "1.2.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./core": {
      "import": "./dist/core.js",
      "types": "./dist/core.d.ts",
      "default": "./dist/core.js"
    },
    "./ipc": {
      "import": "./dist/ipc.js",
      "types": "./dist/ipc.d.ts",
      "default": "./dist/ipc.js"
    }
  },
  "typesVersions": {
    "*": {
      "core": [
        "./dist/vite.d.ts"
      ],
      "ipc": [
        "./dist/ipc.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "vjit-core": "workspace:^",
    "vjit-ipc": "workspace:^"
  },
  "devDependencies": {
    "tsdown": "^0.15.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
